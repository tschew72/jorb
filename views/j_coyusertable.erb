<script>
$.getScript("_/js/loading_scripts/coyusertablescript.js");
</script>

<table id="coyusertable" class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
       <th>Name</th>
       <th>Username</th>
       <th>Contact Number</th>
       <th></th>
       <th></th>
    </tr>
  </thead>
  <tbody >
    <%a=" "%>
   <% @coyusers.each do |i| %>
    <% if i.usertype == 2  && i.status !=2%>  
      <tr>
        <td><%= i.firstname + a + i.lastname%></td>
        <td><%=i.username%></td>
        <td><%=i.contactnumber%></td>
        <td>
          <a onclick="showcoyuserdetail(<%=i.id%>)" href="javascript:void(0)"> <i class="fa fa-external-link"></i>Edit</a>
        </td>
        <td>
          <% if i.accesslevel != 3 %> <!-- No one can delete superadmin -->
            <a onclick="delcoyuser(this, <%=i.id%>)" href="javascript:void(0)"> 
              <i class="fa fa-trash-o fa-lg"></i>
            </a>
          <% end %>
        </td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table> <!--coyusertable -->