
<div class="row">
  <div class="col-lg-11">
  <table class="table table-striped table-hover margin-0px airtable">
    <thead>
      <tr>
        <th >description</th>
        <th >job spec </th>
        <th >seeker detail</th>
        <th >score</th>
      </tr>
    </thead>
    <tbody>
      <% @skrdetail.each do |i| %>
      <tr>
        <td><span class="text-center"> <%= i.fieldname %></span></td>                       
        <td><span class="text-center"> <%= i.jobvalue %></span></td>
        <td><span class="text-center"> <%= i.skrvalue %></span></td>
        <td><span class="text-center"> <%= i.score %></span></td>
      </tr>
      <% end %>
    </tbody>
    
  </table>
</div>
</div>


