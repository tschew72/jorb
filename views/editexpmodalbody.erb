  <input type="checkbox" name="currentjob" id="e_currentjobcheck"  <%= @exp.skr_emp_currentjob ? "checked" : "unchecked"%> > 
  <label for="e_currentjobcheck"></label> 
  <a class="editjob" id="e_skr_emp_currentjob" href="#" data-emptytext="Select..." data-type="select" data-name="skr_emp_currentjob" data-value=<%= @exp.skr_emp_currentjob%>></a>
</p>
