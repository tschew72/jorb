<script>
$.getScript("_/js/loading_scripts/admin_seekertablescript.js");

</script>

<table id="admin_seekertable" class="table table-striped table-bordered table-hover">
  <thead>
  <tr>
   <th>First Name</th>
   <th>Last Name</th>
   <th>UserName</th>
   <th>Last Updated</th>
   <th></th>
   <th></th>
  </tr>
  </thead>
  <tbody >
    <% @allusers.each do |i| %>                
    <% if i.status != 0 %>
    <tr>
      <td><%= i.firstname %></td>
      <td><%= i.lastname %></td>
      <td><%= i.username != nil ? i.username : " "%></td>
      <td><%= i.updated_at %></td>
      <td>
        <a href="/admin_editseekerprofile?pk=<%=i.id%>"> Edit Job Seeker Profile</a>
      </td>
      <td>
        <a href="/admin_editseekercareer?pk=<%=i.id%>"> Edit Career Profile</a>
      </td>
    </tr>
      <% end %>
      <% end %>
  </tbody>
</table>